{
  "name": "scaleway-api-client",
  "version": "1.0.1",
  "description": "NodeJS API Client for Scaleway",
  "main": "lib.js",
  "scripts": {
    "release": "release-it",
    "build": "node build.js",
    "doc": "jsdoc -c ./jsdoc.json"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "after:bump": "npm run build && npm run doc && git add docs/* && git commit -m 'doc: updated'"
    }
  },
  "files": [
    "README.md",
    "lib.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FGRibreau/scaleway-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/FGRibreau/scaleway-api-client/issues"
  },
  "homepage": "https://github.com/FGRibreau/scaleway-api-client",
  "keywords": [
    "scaleway",
    "api",
    "client",
    "cloud"
  ],
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "engines": {
    "node": "12"
  },
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.20.0"
  },
  "devDependencies": {
    "docdash": "^1.2.0",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.5",
    "prettier": "^2.1.1",
    "release-it": "^14.0.2",
    "yaml": "^1.10.0"
  }
}
